root    ::= ws object ws
ws      ::= [ \t\n\r]*

object  ::= "{" ws members? ws "}"
members ::= pair more_pairs
more_pairs ::= ws "," ws pair more_pairs
            | /* empty */

pair    ::= operation_pair
pair    ::= term_pair
pair    ::= filter_pair
pair    ::= region_pair

operation_pair ::= "\"Operation\"" ws ":" ws "\"LOCATE\""
term_pair      ::= "\"Term\""      ws ":" ws string
filter_pair    ::= "\"Filter\""    ws ":" ws string
region_pair    ::= "\"Region\""    ws ":" ws string

string  ::= "\"" chars "\""
chars   ::= char*
char    ::= [^"\\] | escape
escape  ::= "\\" ["\\/bfnrt] | "\\u" HEX HEX HEX HEX
HEX     ::= [0-9a-fA-F]
